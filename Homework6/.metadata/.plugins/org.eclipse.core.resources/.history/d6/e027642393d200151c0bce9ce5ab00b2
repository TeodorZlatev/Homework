package Task2;

public class Fish extends Animal{

	public Fish(String name, int age, boolean isHungry) {
		super(name, age, isHungry);
		
	}
	
	public void setFood(){
		hasFood = true;
	}

	public void eating() {
		System.out.printf("Куче %s изяде храната си.", this.getName());
		hasFood = false;
	}

}
