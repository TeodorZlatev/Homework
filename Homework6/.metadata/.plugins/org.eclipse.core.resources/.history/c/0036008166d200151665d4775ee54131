package Task1;

public class Square extends Figure {
	int side;

	public void calculatePerimeter() {
		System.out.printf("Периметърът на квадрата е 4.%d = %d", side, (4 * side));

	}

	public void calculateArea() {
		System.out.printf("Площта на квадрата е %1$d.1$%d = %d", side, (side * side));

	}

}
