package Task2;

public class Fish extends Animal{

	public Fish(String name, int age, boolean isHungry, boolean hasFood) {
		super(name, age, isHungry, hasFood);
		// TODO Auto-generated constructor stub
	}
	
	public void setFood(){
		this.isThereFood(true);
	}

	public void eating() {
		System.out.printf("Риба %s изяде храната си.", this.getName());
		this.isThereFood(false);
	}


}
