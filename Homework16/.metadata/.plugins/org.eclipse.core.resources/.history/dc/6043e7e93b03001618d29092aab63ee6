package VideoStore;

public class User {
	private int id;
	private int filmsCounter;

	public int getId() {
		return id;
	}
	
	public void setId(int id) {
		this.id = id;
	}

	public int getFilmsCounter() {
		return filmsCounter;
	}

	public void setFilmsCounter(int filmsCounter) {
		this.filmsCounter = filmsCounter;
	}

}
